  <clojure-core-memoize-version>1.0.257</clojure-core-memoize-version>
  <clojure-math-numeric-tower-version>0.0.5</clojure-math-numeric-tower-version>
  <clojure-maven-version>1.9.2</clojure-maven-version>
  <clojure.vmargs>-ea -dsa</clojure.vmargs>
  <clojure.options>-ea -dsa -Dclojure.compiler.direct-linking=true</clojure.options>
  <clojurecl-version>0.15.1</clojurecl-version>
  <commons-compress-version>1.23.0</commons-compress-version>
  <commons-lang3-version>3.12.0</commons-lang3-version>
  <commons-math3-version>3.6.1</commons-math3-version>
  <commons-rng-version>1.5</commons-rng-version>
  <codox-version>0.10.8</codox-version>
  <criterium-version>0.4.6</criterium-version>
  <doclint>none</doclint>
  <eclipse-collections-version>11.1.0</eclipse-collections-version>
  <exif-processor-version>0.3.0</exif-processor-version>
  <fastutil-version>8.5.12</fastutil-version>
  <fipp-version>0.6.26</fipp-version>
  <github-url>scm:git:https://github.com/${project.groupId}/${project.artifactId}.git</github-url>
  <graph-maven-version>1.2.3</graph-maven-version>
  <guava-version>32.1.1-jre</guava-version>
  <hiccup-version>1.0.5</hiccup-version>
  <hppc-version>0.9.1</hppc-version>
  <java-version>20</java-version>
  <java-sizeof-version>0.0.5</java-sizeof-version>
  <jgit-version>6.6.0.202305301015-r</jgit-version>
  <jcuda-version>11.8.0</jcuda-version>
  <jscience-version>4.3.3</jscience-version>
  <junit-jupiter-version>5.10.0</junit-jupiter-version>
  <junit-platform-version>1.10.0</junit-platform-version>
  <jts-version>1.19.0</jts-version>
  <lifecycle-mapping-version>1.0.0</lifecycle-mapping-version>
  <maven-assembly-version>3.6.0</maven-assembly-version>
  <maven.build.timestamp.format>yyyy-MM-dd HH:mm</maven.build.timestamp.format>
  <maven-clean-version>3.3.1</maven-clean-version>
  <maven-compiler-version>3.11.0</maven-compiler-version>
  <maven-dependency-version>3.6.0</maven-dependency-version>
  <maven-deploy-plugin-version>3.1.1</maven-deploy-plugin-version>
  <maven-enforcer-version>3.3.0</maven-enforcer-version>
  <maven-install-plugin-version>3.1.1</maven-install-plugin-version>
  <maven-jar-version>3.3.0</maven-jar-version>
  <maven-javadoc-version>3.5.0</maven-javadoc-version>
  <maven-project-info-reports-version>3.4.5</maven-project-info-reports-version>
  <maven-release-plugin-version>3.0.1</maven-release-plugin-version>
  <maven-resources-plugin-version>3.3.1</maven-resources-plugin-version>
  <maven-scm-plugin-version>2.0.1</maven-scm-plugin-version>
  <maven-site-version>4.0.0-M9</maven-site-version>
  <maven-source-plugin-version>3.3.0</maven-source-plugin-version>
  <maven-surefire-version>3.1.2</maven-surefire-version>
  <metadata-extractor-version>2.18.0</metadata-extractor-version>
  <numbers-java-version>1.8.2</numbers-java-version>
  <oshi-version>6.4.4</oshi-version>
  <pandect-version>1.0.2</pandect-version>
  <parboiled-java-version>1.4.1</parboiled-java-version>
  <potemkin-version>0.4.6</potemkin-version>
  <slf4j-version>2.0.7</slf4j-version>
  <spire_2_12_version>0.17.0</spire_2_12_version>
  <spire_2_13_version>0.18.0</spire_2_13_version>
  <timestamp>${maven.build.timestamp}</timestamp>
  <uncommons-maths-version>1.2.3</uncommons-maths-version>
  <versions-maven-plugin-version>2.16.0</versions-maven-plugin-version>
