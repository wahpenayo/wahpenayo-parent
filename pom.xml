 <properties>
  <doclint>none</doclint>
  <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  <apfloat-version>1.11.1</apfloat-version>
  <asm-version>9.5</asm-version>
  <cheshire-version>5.11.0</cheshire-version>
  <clatrix-version>0.5.0</clatrix-version>
  <clj-tuple-version>0.2.2</clj-tuple-version>
